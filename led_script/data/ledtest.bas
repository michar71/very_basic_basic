NUMLED = GETMAXLED(0)

DIM LEDR(NUMLED)
DIM LEDG(NUMLED)
DIM LEDB(NUMLED)

FORMAT "LEDCOUNT: %",NUMLED

FOR R=0 TO 10
	FORMAT "OL: %",R
	FOR I=1 TO NUMLED
		FORMAT "LED: %",I
		S = -1
		S= ABS(S)
		FORMAT "VAL: %",S
		S = SETARRAY(LEDR,1,NUMLED,0)
		S = SETARRAY(LEDG,1,NUMLED,0)
		LEDR(I) = 255
		LEDG(I) = R * 25
		S = SETLEDRGB(LEDR,LEDG,LEDB)
		S = WAIT(10)
	END FOR
END FOR
S = SETARRAY(LEDR,1,NUMLED,0)
S = SETARRAY(LEDG,1,NUMLED,0)
S = SETARRAY(LEDB,1,NUMLED,0)
S = SETLEDRGB(LEDR,LEDG,LEDB)
FORMAT "RUN DONE"
